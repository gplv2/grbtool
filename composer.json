{
    "name": "laravel/laravel",
    "description": "The Laravel Framework- GRB Tool/Api",
    "keywords": ["framework", "laravel", "Byteless", "GRB GIMT API", "GRBAPI" ],
    "license": "GPL-3.0+",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "tymon/jwt-auth": "0.5.*",
        "dingo/api": "1.0.*@dev",
        "barryvdh/laravel-cors": "^0.8.2",
        "predis/predis": "^1.1",
        "laravelcollective/html": "^5.2",
        "laravel/socialite": "^2.0",
        "phaza/laravel-postgis": "^3.1",
        "fideloper/proxy": "^3.3",
        "swiftmailer/swiftmailer": "^5.2",
        "jbelien/oauth2-openstreetmap": "^0.1.2",
        "league/oauth2-client": "^2.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "app/Port/composer.json",
                "app/Containers/composer.json",
                "app/Containers/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false
        },
        "installer-types": [
          "porto-container"
        ],
        "installer-paths": {
          "app/Containers/{$name}/": [
            "type:porto-container"
          ],"vendor/{$name}/": [
            "type:laravel-library"
          ]
        }
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": [
                "app"
            ]
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
        "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan jwt:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan config:clear",
            "php artisan key:generate",
            "npm --loglevel silent install --production"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan config:clear",
            "php artisan clear-compiled",
            "php artisan optimize",
            "npm install"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
